<h1>Contactez-nous</h1>

<form>
  <div class="form-group">
    <label for="exampleFormControlInput1">Votre adresse</label>
    <input type="email" class="form-control" id="exampleFormControlInput1" placeholder="name@example.com">
  </div>
  <div class="form-group">
    <label for="exampleFormControlSelect1">Le motif de votre demande</label>
    <select class="form-control" id="exampleFormControlSelect1">
      <option>Motif 1</option>
      <option>Motif 2</option>
      <option>Motif 3</option>
      <option>Motif 4</option>
      <option>Motif 5</option>
    </select>
  </div>
  <div class="form-group">
    <label for="exampleFormControlTextarea1">Votre message</label>
    <textarea class="form-control" id="exampleFormControlTextarea1" rows="3"></textarea>
  </div>
</form>

<button type="button" class="btn btn-dark">Soumettre votre demande</button>
